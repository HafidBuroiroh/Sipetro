@model IEnumerable<Login.Models.Master_Data>

@{
    ViewData["title"] = "Transfer Data";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2 class="mt-5">Transfer Data</h2>
<hr />
<form asp-action="Search" asp-antiforgery="true" id="non" method="post">
<div class="row mt-4">
    <div class="col-md-4">
        <div class="input-group mb-3">
            <span class="input-group-text" id="inputGroup-sizing-default">From</span>
            <input name="from" type="date" value="@ViewData["CurrentFilter"]" class="form-control" aria-label="Sizing example input" aria-describedby="inputGroup-sizing-default">
        </div>
    </div>
    <div class="col-md-4">
        <div class="input-group mb-3">
            <span class="input-group-text" id="inputGroup-sizing-default">Until</span>
            <input name="until" type="date"  value="@ViewData["CurrentFilters"]" class="form-control" aria-label="Sizing example input" aria-describedby="inputGroup-sizing-default">
        </div>
    </div>
</div>
    <button name="btnsubmit" type="submit" onclick="openModal()" value="Transfer" class="btn btn-sm btn-success fw-bold" data-bs-toggle="modal" data-bs-target="#exampleModal">Transfer</button>
    <input name="btnsubmit" type="submit" value="Search" class="btn btn-sm btn-success fw-bold"  style="margin-left:10px;">

    <!--Modal-->
    <div class="modal fade" id="exampleModal" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true" data-backdrop="static" data-keyboard="false">
        <div class="modal-dialog modal-dialog-centered">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalLabel">Modal title</h5>
                    <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                </div>
                <div class="modal-body">
                    Transfer Completed
                </div>
                <div class="modal-footer">
                    <button type="submit" class="btn btn-danger" data-bs-dismiss="modal">Close</button>
                </div>
            </div>
        </div>
    </div>
</form>



